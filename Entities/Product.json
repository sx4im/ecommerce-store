{
  "name": "Product",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Product name"
    },
    "description": {
      "type": "string",
      "description": "Product description"
    },
    "price": {
      "type": "number",
      "description": "Product price"
    },
    "image_url": {
      "type": "string",
      "description": "Product image URL"
    },
    "category": {
      "type": "string",
      "enum": [
        "jewelry",
        "bags",
        "watches",
        "belts",
        "sunglasses",
        "hats",
        "scarves",
        "phone_cases",
        "keychains",
        "wallets"
      ],
      "description": "Product category"
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "description": "Whether product is featured"
    },
    "available": {
      "type": "boolean",
      "default": true,
      "description": "Whether product is available"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Product tags"
    }
  },
  "required": [
    "name",
    "price",
    "image_url",
    "category"
  ]
}